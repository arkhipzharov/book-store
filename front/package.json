{
  "private": true,
  "name": "front",
  "scripts": {
    "stylelintcache:create-file": "echo $null >> .stylelintcache",
    "stylelint": "stylelint './src/**/*.js' --cache --cache-location './.stylelintcache'; exit 0",
    "stylelint:watch": "nodemon --quiet --watch src -x \"npm run stylelint\"",
    "webpack:dev": "webpack-dev-server --env.development --config webpack.base.js",
    "webpack:prod": "webpack --env.production --config webpack.base.js",
    "purgecss:postbuild": "purgecss --config .purgecssrc.js",
    "dev": "npm-run-all -p webpack:dev stylelint:watch",
    "build": "npm run webpack:prod && npm run purgecss:postbuild"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components-px2rem": "^1.5.5",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.2.0",
    "cssnano": "^4.1.10",
    "directory-named-webpack-plugin": "^4.0.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "fast-sass-loader": "^1.5.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.10.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.32",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "purgecss": "^2.3.0",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "svg-sprite-loader": "^5.0.0",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.2.1",
    "terser-webpack-plugin": "^3.0.8",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap-scss": "^4.5.2",
    "bootstrap-slider": "^11.0.2",
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-avatar-edit": "^0.8.3",
    "react-bootstrap": "^1.3.0",
    "react-bootstrap-slider": "^3.0.0",
    "react-bootstrap-typeahead": "^5.1.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.4.0",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-star-rating-component": "^1.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.1.1"
  }
}
