{
  "private": true,
  "scripts": {
    "back": "cd back && npm run app",
    "dev": "cd front && npm run dev",
    "build": "cd front && npm run build",
    "deps": "npm install && npx lerna bootstrap",
    "setup": "npm run deps && cd front && npm run stylelintcache:create-file",
    "i": "lerna add",
    "un": "lerna exec -- npm uninstall",
    "lint": "./node_modules/.bin/eslint . -c .eslintrc.js --quiet --ext .js",
    "lint:fix": "./node_modules/.bin/eslint . -c .eslintrc.js --quiet --fix --ext .js"
  },
  "devDependencies": {
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lerna": "^3.22.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "lodash": "^4.17.20"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 6.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix && git add ."
    }
  }
}
