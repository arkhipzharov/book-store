    "app": "webpack-dev-server --env.development --config webpack.base.js",
    "dev": "npm-run-all -p app stylelint:watch",
    "build": "webpack --env.production --config webpack.base.js && npm run purgecss:postbuild",
    "setup": "npm install && echo $null >> .stylelintcache",
    "purgecss:postbuild": "purgecss --config .purgecssrc.js"
